#! /usr/bin/env bash

set -e # stop if it fails at any point

GREEN='\033[1;32m'
NO_COLOR='\033[0m'
LABEL="bootstrap"
printf "${GREEN}====== ${LABEL}${NO_COLOR}\n"

ABS_PATH=$(readlink -f "$0")
BIN_DIR=$(dirname $ABS_PATH)

source "$BIN_DIR/db/setup"
source "$BIN_DIR/ddb/schema-load"
source "$BIN_DIR/ddb/seed"